{
  "name": "next-14",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7",
    "yarn": "please use npm",
    "pnpm": "please use npm",
    "pn": "please use npm"
  },
  "scripts": {
    "dev": "run-p dev:next dev:scss",
    "dev:next": "next dev",
    "dev:scss": "typed-scss-modules src --watch",
    "typegen:scss": "typed-scss-modules src",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all -p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint . --ext \".js,.ts,.jsx,.tsx\"",
    "lint:prettier": "prettier --check .",
    "lint:types": "tsc --pretty --noEmit",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "fix": "run-s fix:prettier fix:eslint fix:style",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix:style": "stylelint --fix '**/*.{css,scss,sass}'",
    "swagger": "openapi --input https://dev.sourcemaker.ninja/swagger/ --output ./src/init/swagger/ --client fetch --name SwaggerClient --useUnionTypes --useOptions"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.2",
    "axios": "^1.6.2",
    "next": "14.0.4",
    "next-auth": "^5.0.0-beta.4",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.25.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "swr": "^2.2.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "orval": "^6.23.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "stylelint": "^16.0.2",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^35.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5",
    "webpack": "^5.89.0"
  }
}
